package br.com.fiap.ws.entiy;

public class Cliente {

	private int codigo;
	private String nome;
	private String emai;
	
	public Cliente() {
	}
	public Cliente(int codigo, String nome, String emai) {
		super();
		this.codigo = codigo;
		this.nome = nome;
		this.emai = emai;
	}

	public int getCodigo() {
		return codigo;
	}
	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getEmai() {
		return emai;
	}
	public void setEmai(String emai) {
		this.emai = emai;
	}
	
	
	
	
}
