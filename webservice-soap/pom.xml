<project xmlns="http://maven.apache.org/POM/11.0.0" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://maven.apache.org/POM/11.0.0 
  http://maven.apache.org/xsd/maven-11.0.0.xsd">
<modelVersion>11.0.0</modelVersion>
<groupId>com.fiap.ws</groupId>
<artifactId>OlaClienteWS</artifactId>
<version>0.0.1-SNAPSHOT</version>
<dependencies>
  <dependency>
    <groupId>org.apache.cxf</groupId>
      <artifactId>
        cxf-rt-frontend-jaxws
      </artifactId>
    <version>3.2.7</version>
  </dependency>
  <dependency>
    <groupId>org.apache.cxf</groupId>
      <artifactId>
        cxf-rt-transports-http
      </artifactId>
    <version>3.2.7</version>
  </dependency>
  <dependency>
    <groupId>org.apache.cxf</groupId>
    <artifactId>
      cxf-rt-transports-http-jetty
    </artifactId>
    <version>3.2.7</version>
  </dependency>
</dependencies>
<build>
  <sourceDirectory>src</sourceDirectory>
  <plugins>
    <plugin>
      <artifactId>
        maven-compiler-plugin
      </artifactId>
      <version>3.8.0</version>
      <configuration>
        <source>1.8</source>
        <target>1.8</target>
      </configuration>
    </plugin>
  </plugins>
</build>
</project>